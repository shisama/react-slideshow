{
  "name": "react-slideshow-ui",
  "version": "1.0.4",
  "author": "Masashi Hirano (https://github.com/shisama)",
  "license": "MIT",
  "description": "React UI Component for slideshow like SlideShare or SpeakerDeck.",
  "main": "lib",
  "homepage": "https://github.com/shisama/react-slideshow-ui",
  "bugs": {
    "url": "https://github.com/shisama/react-slideshow-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shisama/react-slideshow-ui.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "slideshow",
    "slide",
    "slides",
    "slideshare",
    "speakerdeck",
    "presentation",
    "keynote",
    "powerpoint"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "build:demo": "NODE_ENV=development webpack",
    "build:dll": "NODE_ENV=production webpack --config webpack.dll.config.js",
    "build:all": "npm run lint && npm test && npm run build:dll && npm run build:demo && npm run clean && npm run build",
    "clean": "rimraf lib",
    "test": "NODE_ENV=test jest --verbose --coverage",
    "lint": "tslint --project . --fix",
    "prepublishOnly": "npm test && npm build",
    "git-add": "git add lib demo dll",
    "check-updates": "ncu"
  },
  "types": "index.d.ts",
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0"
  },
  "dependencies": {
    "react": "^16.6.0",
    "recompose": "^0.30.0",
    "toggle-fullscreen": "^0.3.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/recompose": "^0.27.0",
    "@types/sinon": "^5.0.5",
    "browser-env": "^3.2.5",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "npm-check-updates": "^2.14.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-dom": "^16.6.0",
    "react-hot-loader": "4.3.12",
    "react-test-renderer": "^16.6.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.1.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  },
  "pre-commit": [
    "build:all",
    "git-add"
  ]
}
