{
  "name": "react-slideshow-ui",
  "version": "1.0.4",
  "author": "Masashi Hirano (https://github.com/shisama)",
  "license": "MIT",
  "description": "React UI Component for slideshow like SlideShare or SpeakerDeck.",
  "main": "lib/SlideShow.js",
  "homepage": "https://github.com/shisama/react-slideshow-ui",
  "bugs": {
    "url": "https://github.com/shisama/react-slideshow-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shisama/react-slideshow-ui.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "slideshow",
    "slide",
    "slides",
    "slideshare",
    "speakerdeck",
    "presentation",
    "keynote",
    "powerpoint"
  ],
  "scripts": {
    "build": "npm run clean && babel --no-comments src --out-dir lib --ignore src/demo",
    "build:demo": "NODE_ENV=development webpack",
    "build:dll": "NODE_ENV=production webpack --config webpack.dll.config.js",
    "build:all": "npm run lint && npm run typecheck && npm test && npm run build:dll && npm run build:demo && npm run clean && npm run build",
    "clean": "rimraf lib",
    "test": "NODE_ENV=test jest --verbose --coverage",
    "lint": "eslint src --ext js,jsx --fix",
    "typecheck": "flow",
    "remove-types": "flow-remove-types --out-dir out/ src/ && eslint out --ext js,jsx --fix",
    "prepublishOnly": "npm test && npm build",
    "git-add": "git add lib demo dll",
    "typed": "flow-typed install --skip",
    "check-updates": "ncu"
  },
  "dependencies": {
    "react": "^16.6.0",
    "recompose": "^0.30.0",
    "toggle-fullscreen": "^0.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-flow": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "browser-env": "^3.2.5",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-closure": "^0.1.1",
    "eslint-config-google": "^0.11.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.85.0",
    "flow-remove-types": "^1.2.1",
    "flow-typed": "^2.5.1",
    "jest": "^23.6.0",
    "npm-check-updates": "^2.14.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-dom": "^16.6.0",
    "react-hot-loader": "4.3.12",
    "react-test-renderer": "^16.6.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.1.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions"
                ]
              }
            }
          ],
          "@babel/preset-react",
          "@babel/preset-flow"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-object-assign"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "IE 11"
                ]
              }
            }
          ],
          "@babel/preset-react",
          "@babel/preset-flow"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-object-assign"
        ]
      },
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          "@babel/preset-react",
          "@babel/preset-flow"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-object-assign"
        ]
      }
    }
  },
  "pre-commit": [
    "build:all",
    "git-add"
  ]
}
